{
    "name": "NodeJS",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "runArgs": [
        "--cap-add=SYS_PTRACE",
        "--security-opt",
        "seccomp=unconfined"
    ],
    "settings": {},
    "extensions": [
        "dbaeumer.vscode-eslint"
    ],
    "forwardPorts": [
        3000
    ],
    "portsAttributes": {
        "3000": {
            "label": "Hello Remote World",
            "onAutoForward": "notify"
        }
    },
    "postCreateCommand": "npm install",
    "remoteUser": "node"
}